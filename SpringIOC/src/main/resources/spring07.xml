<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--autowire="default" 不自动装配-->
    <!--autowire="no" 不自动装配-->
    <!--autowire="byName" 不自动装配-->
    <!--autowire="byType" 不自动装配-->
    <!--autowire="constructor" 不自动装配-->
    <bean id="student" class="com.web.beans.Student" autowire="byName">
        <property name="name" value="小明"></property>
    </bean>

    <bean id="homework" class="com.web.beans.Homework">
        <property name="content" value="how to calc 3+2 ?"></property>
    </bean>

</beans>